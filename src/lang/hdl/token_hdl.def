#include "../core/token_begin.def"

/*================================*/
/*                                */
/*        TOKENS DEFINITION.      */
/*                                */
/*================================*/

/**
 * Grouping.
 */
SYMBOL_TOKEN(LBrace,  "{")
SYMBOL_TOKEN(RBrace,  "}")
SYMBOL_TOKEN(LParen,  "(")
SYMBOL_TOKEN(RParen,  ")")
SYMBOL_TOKEN(LSqaure, "[")
SYMBOL_TOKEN(RSquare, "]")

SYMBOL_TOKEN(Comma,      ",")
SYMBOL_TOKEN(Semicolon,  ";")
SYMBOL_TOKEN(Colon,      ":")
SYMBOL_TOKEN(Assignment, "=")

/**
 * Keywords.
 */
KEYWORD_TOKEN(Chip,  "CHIP")
KEYWORD_TOKEN(In,    "IN")
KEYWORD_TOKEN(Out,   "OUT")
KEYWORD_TOKEN(Parts, "PARTS")

#include "../core/token_end.def"
