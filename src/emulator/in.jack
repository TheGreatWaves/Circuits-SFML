class Main {
    function int main() {
		do Counter.set(-10);
		do Counter.increment();
		do Counter.increment();
		do Counter.increment();
		return Counter.value();
    }
}

class Counter {
	static int counter;

	function void increment() {
		let counter = counter + 1;
	}

	function void set(int value) {
		let counter = value;
	}

	function int value() {
		return counter;
	}
}
